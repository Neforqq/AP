import java.lang.Math
public class xy_formyla {
  public static void main(String[] args) {
double c = 0.7;
double m = 3 *  Math.pow(10, -3);
double n = 1.2;
double a = 5;
// обчислення х та у  за формулами 
double x = a / Math.sqrt(1 + c * Math.exp(-m));
double y = a * (1 - (Math.cos(c) / (Math.pow(n, 2) - Math.pow(Math.sin(c), 2))));

System.out.println("x =" + x);
System.out.println("y =" + y);
   }  
}

